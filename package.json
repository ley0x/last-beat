{
  "name": "last-beat",
  "version": "0.1.0",
  "private": false,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "type-check": "tsc --noEmit",
    "dev": "vite dev",
    "build": "vite build && tsc --noEmit",
    "start": "node .output/server/index.mjs",
    "lint": "eslint src --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "dev:up": "podman compose -f compose.dev.yml up -d",
    "dev:build": "podman compose -f compose.dev.yml up --build -d",
    "dev:down": "podman compose -f compose.dev.yml down",
    "dev:log": "podman compose -f compose.dev.yml logs -f",
    "dev:shell": "podman exec -ti app-dev /bin/sh",
    "prod:up": "podman compose -f compose.yml up --build -d",
    "prod:down": "podman compose -f compose.yml down",
    "prod:log": "podman compose -f compose.yml logs -f",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  },
